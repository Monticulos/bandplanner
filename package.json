{
  "name": "bandplanner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./frontend/config/vite.config.ts",
    "build": "tsc --build ./frontend/config/tsconfig.json && vite build --config ./frontend/config/vite.config.ts",
    "lint": "eslint --config ./frontend/config/eslint.config.js",
    "preview": "vite preview --config ./frontend/config/vite.config.ts",
    "test": "vitest --config ./frontend/config/vitest.config.js",
    "typecheck": "tsc --project ./frontend/config/tsconfig.json",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "prettier": "3.4.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.0.11",
    "vitest": "^3.0.5"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
